{
  "openapi": "3.0.0",
  "info": {
    "title": "Your API Title",
    "version": "1.0.0",
    "description": "Description of your API"
  },
  "servers": [
    {
      "url": "http://localhost:3000",
      "description": "Development server"
    }
  ],
  "paths": {
    "/gioTags": {
      "get": {
        "summary": "Get all GioTags",
        "description": "Retrieve a list of GioTags",
        "responses": {
          "200": {
            "description": "A list of GioTags",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GioTagList"
                }
              }
            }
          }
        }
      },
      "post": {
        "summary": "Create a new GioTag",
        "description": "Add a new GioTag",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/GioTag"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "GioTag created successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GioTag"
                }
              }
            }
          }
        }
      }
    },
    "/gioTags/{id}": {
      "get": {
        "summary": "Get a GioTag by ID",
        "description": "Retrieve a GioTag by its ID",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "A GioTag",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GioTag"
                }
              }
            }
          },
          "404": {
            "description": "GioTag not found"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "GeoLocation": {
        "type": "object",
        "properties": {
          "latitude": {
            "type": "number",
            "format": "float",
            "description": "Latitude of the location"
          },
          "longitude": {
            "type": "number",
            "format": "float",
            "description": "Longitude of the location"
          },
          "radius": {
            "type": "number",
            "format": "float",
            "description": "Radius around the location"
          }
        },
        "required": ["latitude", "longitude", "radius"]
      },
      "Image": {
        "type": "object",
        "properties": {
          "url": {
            "type": "string",
            "description": "URL of the image"
          },
          "description": {
            "type": "string",
            "description": "Description of the image"
          }
        }
      },
      "Audio": {
        "type": "object",
        "properties": {
          "url": {
            "type": "string",
            "description": "URL of the audio"
          },
          "description": {
            "type": "string",
            "description": "Description of the audio"
          }
        }
      },
      "GioTag": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "Name of the GioTag"
          },
          "address": {
            "type": "string",
            "description": "Address of the GioTag"
          },
          "phoneNumber": {
            "type": "string",
            "description": "Phone number associated with the GioTag"
          },
          "adults": {
            "type": "integer",
            "description": "Number of adults"
          },
          "healthyAdults": {
            "type": "integer",
            "description": "Number of healthy adults"
          },
          "children": {
            "type": "integer",
            "description": "Number of children"
          },
          "geoLocation": {
            "$ref": "#/components/schemas/GeoLocation"
          },
          "images": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Image"
            }
          },
          "audio": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Audio"
            }
          }
        },
        "required": ["name", "phoneNumber", "geoLocation"]
      },
      "GioTagList": {
        "type": "array",
        "items": {
          "$ref": "#/components/schemas/GioTag"
        }
      }
    }
  }
}
